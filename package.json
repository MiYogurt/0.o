{
    "name": "yugo-utils",
    "version": "0.0.2",
    "description": "Awesome Utils",
    "main": "dist/index.js",
    "scripts": {
        "build": "rm -rf dist && tsc",
        "test": "jest"
    },
    "type": "index.d.ts",
    "files": [
        "dist"
    ],
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "keywords": [
        "utils"
    ],
    "author": "ChenMingMing",
    "bugs": {
        "url": "https://github.com/MiYogurt/0.o/issues"
    },
    "repository": "miyogurt/0.o",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^23.1.3",
        "@types/node": "^10.5.1",
        "jest": "^23.2.0",
        "ts-jest": "^23.0.0",
        "typescript": "^2.9.2"
    }
}